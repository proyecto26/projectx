{
  "name": "@projectx/models",
  "version": "0.0.1",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --outDir dist && tsc --emitDeclarationOnly --declaration --declarationMap --outDir dist",
    "dev": "tsc -w -p tsconfig.json",
    "lint": "biome check .",
    "test": "echo 'no tests'"
  },
  "dependencies": {
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "tslib": "^2.8.1",
    "@2toad/profanity": "3.2.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@nestjs/common": "11.1.12",
    "@nestjs/config": "4.0.2",
    "@nestjs/swagger": "11.2.5"
  }
}