{
  "name": "auth",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/auth/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/auth",
        "main": "apps/auth/src/main.ts",
        "tsConfig": "apps/auth/tsconfig.app.json",
        "assets": [
          "apps/auth/src/assets",
          "apps/auth/src/workflows"
        ],
        "poll": 500,
        "webpackConfig": "apps/auth/webpack.config.js"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "auth:build:development",
        "watch": true,
        "waitUntilTargets": [
          "@projectx/core:build",
          "@projectx/db:build",
          "@projectx/models:build",
          "@projectx/email:build",
          "@projectx/workflows:build"
        ]
      },
      "configurations": {
        "development": {
          "buildTarget": "auth:build:development"
        },
        "production": {
          "buildTarget": "auth:build:production"
        }
      }
    }
  }
}